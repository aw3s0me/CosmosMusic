@model IEnumerable<CosmosMusic.Models.Artists>

@{
    ViewBag.Title = "Browse Genre";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 style="color: black">Browsing Genre: @Request.Params["genre"]</h2>
<br />

@foreach (var artist in Model)
{
 <!--   <li>@Html.ActionLink(artist.name, "Albums", new { artist = artist.artist_id.ToString()} )</li> -->
    <li><a href="@Url.Action("Albums","Music", new { @id = artist.artist_id.ToString()} )">@artist.name</a></li>
}
