@model IEnumerable<CosmosMusic.Models.Genre>

@{
    ViewBag.Title = "Albums index";
}

<h2>Browsing Genres:</h2>
<br />

<div class="music" >
    <ul>
        @foreach (var genre in Model)
        {
            <li>@Html.ActionLink(genre.name, "Browse", new {genre = genre.name})</li>
        }
    </ul>
</div>