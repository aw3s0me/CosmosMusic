@model IEnumerable<CosmosMusic.Models.Artists>

@{
    ViewBag.Title = "Browse Genre";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 style="color: white">Albums index</h2>
<br />

@foreach (var artist in Model)
{
    <li>@Html.ActionLink(artist.name, "Albums", new { artist = artist.artist_id} )</li>
}
